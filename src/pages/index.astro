---
import Main from '../components/Main.astro'
import Space from '../components/Space.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';

let title = 'My Astro Site';
---
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width">
	<title>{title}</title>
	<link rel="icon" type="image/x-icon" href="/favicon.ico" />
	<style global lang="scss">
			@import "../styles/global.scss";
	</style>
</head>
<body>
	<Space />
    <Header />
	<Main>
		<section class="body-font">
			<div class="content-center text-center h-screen justify-center pt-15">
				<h1 class="pt-10 text-6xl font-bold font-weight-1000 bg-gradient-to-r from-cyan-500 to-blue-500 text-transparent bg-clip-text">Hello! I'm MostlyWhat</h1>
				<p class="px-5">euhfeiuhwfhuiwhuifhueiwhhfuih</p>
			</div>
		</section>
		<section class="body-font">
		<div class="container px-5 py-24 mx-auto">
			<div class="flex flex-col">
			<div class="h-1 bg-gray-200 rounded overflow-hidden">
				<div class="w-24 h-full bg-indigo-500"></div>
			</div>
